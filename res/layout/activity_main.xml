<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/yellow"
    tools:context="com.example.ui.MainActivity"
    tools:ignore="MergeRootFrame" >
    
 <LinearLayout
     android:layout_width="match_parent"
     android:layout_height="111dp"
     android:layout_marginTop="30dp"
     android:orientation="horizontal" >

     <ImageView
         android:id="@+id/th"
         android:layout_width="wrap_content"
         android:layout_height="132dp"
         android:background="@drawable/th"
         android:textAppearance="?android:attr/textAppearanceLarge" />
   
     
 </LinearLayout> 
    
    
  <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.17" >

    <Button
        android:id="@+id/Button01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="170dp"
        android:layout_toLeftOf="@+id/Button02"
        android:background="@drawable/onoff"
        android:text="ON" />

    <Button
        android:id="@+id/Button02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Button01"
        android:layout_alignBottom="@+id/Button01"
        android:layout_alignParentRight="true"
        android:layout_marginRight="43dp"
        android:background="@drawable/onoff"
        android:text="OFF" />
    
     <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Button01"
        android:layout_alignBottom="@+id/Button01"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="56dp"
        android:text="JLKom"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Button
        android:id="@+id/Button03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/Button04"
        android:layout_toLeftOf="@+id/Button04"
        android:background="@drawable/onoff"
        android:text="ON" />

    <Button
        android:id="@+id/Button04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Button02"
        android:layout_below="@+id/Button02"
        android:background="@drawable/onoff"
        android:text="OFF" />
    
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Button03"
        android:layout_alignBottom="@+id/Button03"
        android:layout_marginLeft="35dp"
        android:text="실내움직임"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    
    
    <Button
        android:id="@+id/Button05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/Button06"
        android:layout_toLeftOf="@+id/Button06"
        android:background="@drawable/onoff"
        android:text="ON" />

    <Button
        android:id="@+id/Button06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Button04"
        android:layout_below="@+id/Button04"
        android:background="@drawable/onoff"
        android:text="OFF" />
    
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Button05"
        android:layout_alignBottom="@+id/Button05"
        android:layout_marginLeft="55dp"
        android:text="문열림"
        android:textAppearance="?android:attr/textAppearanceLarge" /> 
    
	<Button
        android:id="@+id/Button07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/Button08"
        android:layout_toLeftOf="@+id/Button08"
        android:background="@drawable/onoff"
        android:text="ON" />

    <Button
        android:id="@+id/Button08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Button06"
        android:layout_below="@+id/Button06"
        android:background="@drawable/onoff"
        android:text="OFF" />
   
     <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Button07"
        android:layout_alignBottom="@+id/Button07"
        android:layout_marginLeft="45dp"
        android:text="창문열림"
        android:textAppearance="?android:attr/textAppearanceLarge" /> 
        
	<Button
        android:id="@+id/Button09"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/Button10"
        android:layout_toLeftOf="@+id/Button10"
        android:background="@drawable/onoff"
        android:text="ON" />

    <Button
        android:id="@+id/Button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Button08"
        android:layout_below="@+id/Button08"
        android:background="@drawable/onoff"
        android:text="OFF" />
   
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Button09"
        android:layout_alignBottom="@+id/Button09"
        android:layout_marginLeft="45dp"
        android:text="창문파손"
        android:textAppearance="?android:attr/textAppearanceLarge" /> 
    
   	<Button
        android:id="@+id/Button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/Button12"
        android:layout_toLeftOf="@+id/Button12"
        android:background="@drawable/onoff"
        android:text="ON" />

    <Button
        android:id="@+id/Button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Button10"
        android:layout_below="@+id/Button10"
        android:background="@drawable/onoff"
        android:text="OFF" />
    
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Button11"
        android:layout_alignBottom="@+id/Button11"
        android:layout_marginLeft="60dp"
        android:text="누수"
        android:textAppearance="?android:attr/textAppearanceLarge" /> 
    
    <Button
        android:id="@+id/Button13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/Button14"
        android:layout_toLeftOf="@+id/Button14"
        android:background="@drawable/onoff"
        android:text="ON" />

    <Button
        android:id="@+id/Button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Button12"
        android:layout_below="@+id/Button12"
        android:background="@drawable/onoff"
        android:text="OFF" />
    
    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/Button13"
        android:layout_alignBottom="@+id/Button13"
        android:layout_marginLeft="60dp"
        android:text="화재"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/humidity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/temperature"
        android:layout_alignBottom="@+id/temperature"
        android:layout_alignRight="@+id/Button02"
        android:layout_marginRight="30dp"
        android:text="40%"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/temperature"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Button02"
        android:layout_alignRight="@+id/textView4"
        android:layout_marginBottom="60dp"
         android:layout_marginRight="8dp"
        android:text="25˚C"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    
    </RelativeLayout>
    
    
    
  </FrameLayout>

